{
  "name": "brownie",
  "version": "1.1.2",
  "description": "The resource cleanup application of PIT toolkit",
  "type": "module",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kindredgroup/pit-toolkit.git"
  },
  "author": "kindredgroup",
  "license": "MIT",
  "homepage": "https://github.com/kindredgroup/pit-toolkit#readme",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.8.9",
    "@types/pg-format": "^1.0.5",
    "@types/sinon": "^17.0.2",
    "c8": "^8.0.1",
    "chai": "^4.3.10",
    "esmock": "^2.6.0",
    "mocha": "^10.2.0",
    "node-pg-migrate": "^6.2.2",
    "sinon": "^17.0.1",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && npx tsc",
    "test": "npm run build && c8 --reporter=html --reporter=text mocha --timeout 10000 --recursive dist/test/ --loader=esmock",
    "dev.start": "set -o allexport; source .env ; set +o allexport; npm run build && npm run start",
    "dev.build-image": "source .env; docker build -t $REGISTRY_URL/$SERVICE_NAME:$IMAGE_TAG .",
    "dev.deploy": "./scripts/deploy.sh",
    "start": "node $BROWNIE_NODE_OPTIONS dist/index.js"
  },
  "dependencies": {
    "kafkajs": "^2.2.4",
    "pg": "^8.11.3",
    "pg-format": "^1.0.4",
    "winston": "^3.11.0",
    "yaml": "^2.3.4"
  }
}
